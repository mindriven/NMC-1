{
  "devDependencies": {
    "cross-env": "^5.2.0",
    "flow-bin": "^0.85.0",
    "flow-remove-types": "^1.2.3",
    "nodemon": "^1.18.5"
  },
  "scripts": {
    "build": "flow-remove-types ./ -d out/ -i \"out|node_modules|noop.js\" -x .json,.js",
    "flow:": "flow check ./",
    "start": "nodemon ./out/index.js",
    "hot-build": "nodemon noop.js --config nodemon-rebuilding.json"
  }
}
